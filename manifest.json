{
    "manifest_version": 2,
    "name": "Egret Inspector",
    "description": "Egret Chrome Debug Tool",
    "version": "2.5.4",
    "devtools_page": "devtools.html",
    "key": "",
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": [
                "contentScripts.min.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "scripts": [ "backgroudScripts.min.js" ]
    },
    "permissions": [
        "*://*/*",
        "system.cpu",
        "tabs",
        "storage",
        "nativeMessaging"
    ],
    "web_accessible_resources": [
        "*/*",
        "*"
    ],
    "icons": {
        
        "48": "icon.png"
    }
}
